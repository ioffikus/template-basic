  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/jest/setupEnv.js",
      "<rootDir>/jest/setupEnzyme.js",
      "<rootDir>/jest/matchMediaMock.js",
      "<rootDir>/jest/localStoragePolyfill.js",
      "<rootDir>/jest/windowLocationPolyfill.js"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "presets": [
            "react",
            "es2015",
            "stage-0"
          ],
          "plugins": [
            [
              "import",
              {
                "libraryName": "antd",
                "style": false
              }
            ],
            "styled-jsx/babel"
          ]
        },
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }

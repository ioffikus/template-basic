{
  "name": "template-basic",
  "version": "0.0.3",
  "description": "alicanto basic shop template",
  "main": "",
  "scripts": {
    "test": "npm-run-all lint test-coverage",
    "test-coverage": "jest --coverage",
    "test-simple": "jest",
    "test-for-ci": "jest --maxWorkers 4 --ci --testResultsProcessor=\"jest-junit\"",
    "test-circleci": "npm-run-all lint test-for-ci",
    "lint": "tslint -p tsconfig.test.json -c tslint.json --type-check",
    "watch:server": "webpack --config webpack/server/watch.js",
    "wait:server": "nodemon ./dist/server/server.js --delay 2000ms",
    "watch:client": "webpack-dev-server --config webpack/client/watch.js",
    "watch": "npm-run-all --parallel watch:* wait:server",
    "build": "npm-run-all --parallel build:*",
    "build:client": "webpack --config webpack/client/config.js",
    "build:server": "webpack --config webpack/server/config.js",
    "analyzer:client": "webpack --config webpack/client/analyzer.js",
    "start": "node ./dist/server/server.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --print-width 120 --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
